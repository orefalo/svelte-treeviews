{
	"name": "svelte-treeviews",
	"version": "0.1.0",
	"description": "A full featured treeview component, ported from liquor-tree",
	"author": {
		"name": "Olivier Refalo",
		"url": "https://github.com/orefalo"
	},
	"keywords": [
		"svelte",
		"component",
		"tree",
		"treeview"
	],
	"bugs": "https://github.com/orefalo/svelte-treeviews/issues",
	"homepage": "https://orefalo.github.io/svelte-treeviews/",
	"repository": {
		"type": "git",
		"url": "https://github.com/orefalo/svelte-treeviewss"
	},
	"license": "MIT",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"build-ghpages": "rm -rf docs && svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. src/ && eslint --ignore-path .gitignore src/",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. src/"
	},
	"devDependencies": {
		"@playwright/test": "^1.34.3",
		"@sveltejs/adapter-static": "2.0.2",
		"@sveltejs/kit": "1.20.0",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"eslint": "^8.41.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"nanoid": "^4.0.2",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-highlight": "^7.3.0",
		"svelte-preprocess": "^5.0.4",
		"svelte-range-slider-pips": "^2.1.1",
		"svelte2tsx": "^0.6.15",
		"tslib": "^2.5.2",
		"typescript": "~5.1.3",
		"vite": "^4.3.9"
	},
	"type": "module"
}
