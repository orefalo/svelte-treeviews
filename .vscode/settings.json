{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".husky": true,
    ".nojekyll": true,
    ".commitlintrc.cjs": true,
    ".cz-config.cjs": true,
    ".eslintcache": true,
    ".gitattributes": true,
    ".gitignore": true,
    ".npmrc": true,
    ".prettierignore": true,
    "pnpm-lock.yaml": true,
    "playwright.config.ts": true,
    ".svelte-kit": true,
    ".vscode": true,
    "node_modules": true
  },
  "hide-files.files": [
    ".husky",
    ".nojekyll",
    ".commitlintrc.cjs",
    ".cz-config.cjs",
    ".eslintcache",
    ".gitattributes",
    ".gitignore",
    ".npmrc",
    ".prettierignore",
    "pnpm-lock.yaml",
    "playwright.config.ts",
    ".svelte-kit",
    ".vscode",
    "node_modules"
  ],
  "files.eol": "\n",
  "editor.insertSpaces": false,
  "[typescript]": {
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.tabSize": 4
  },
  "svelte.plugin.svelte.compilerWarnings": {
    "css-unused-selector": "ignore"
  }
}
