{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/Thumbs.db": true,
		"node_modules": true,
		".vscode": true,
		".husky": true
	},
	"hide-files.files": ["node_modules", ".vscode", ".husky"],
	"files.eol": "\n",
	"editor.insertSpaces": false,
	"[typescript]": {
		"editor.tabSize": 4
	},
	"[javascript]": {
		"editor.tabSize": 4
	}
}
