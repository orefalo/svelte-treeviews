{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"verbatimModuleSyntax": true,
		"paths": {
			"$lib": ["./src/lib"],
			"$lib/*": ["./src/lib/*"],
			"svelte-treeviews": ["./src/lib"],
			"svelte-treeviews/*": ["./src/lib/*"],
			"$comp": ["./src/comp"],
			"$comp/*": ["./src/comp/*"]
		}
	}
}
